{
  "schemaVersion": 1,
  "id": "mocolors",
  "version": "${version}",

  "name": "Mo' Colors",
  "description": "Mo Colors adds a painter which can be crafted with two sticks and one wool.",
  "authors": [
    "GabrielOlvH"
  ],
  "contact": {
    "sources": "https://github.com/GabrielOlvH/Mo-Colors"
  },

  "license": "Apache-2.0",
  "icon": "assets/mocolors/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "me.steven.mocolors.MoColors"
    ],
    "client": [
      "me.steven.mocolors.MoColorsClient"
    ],
    "cardinal-components-color": [
      "me.steven.mocolors.MoColors"
    ]
  },
  "custom": {
    "cardinal-components": [
      "mocolors:color"
    ],
    "dashloader:customobject":  [
      "me.steven.mocolors.compat.dash.DashBasicModel",
      "me.steven.mocolors.compat.dash.DashBrickSlabModel",
      "me.steven.mocolors.compat.dash.DashBrickStairsModel",
      "me.steven.mocolors.compat.dash.DashGlassPaneModel",
      "me.steven.mocolors.compat.dash.DashPainterModel"
    ]
  },
  "mixins": [
    "mocolors.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.10.0",
    "fabric": "*",
    "minecraft": "1.18.x"
  }
}
